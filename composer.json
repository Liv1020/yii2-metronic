{
    "name": "wfcreations/yii2-metronic",
    "description": "Yii2 Metronic theme",
    "keywords": [
        "yii2",
        "theme",
        "metronic"
    ],
    "type": "yii2-extension",
    "license": "BSD",
    "support": {
        "issues": "https://github.com/wfcreations/yii2-metronic/issues",
        "wiki": "https://github.com/wfcreations/yii2-metronic/wiki",
        "source": "https://github.com/wfcreations/yii2-metronic"
    },
    "authors": [
        {
            "name": "Welsiton Ferreira",
            "email": "wfcreations@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-jui": "*",
        "wfcreations/yii2-simple-line-icons": "*",
        "wfcreations/yii2-asset-legacy": "dev-master",
        "2amigos/yii2-ckeditor-widget": "*",
        "2amigos/yii2-file-upload-widget": "*",
        "2amigos/yii2-type-ahead-widget": "*",
        "2amigos/yii2-switch-widget": "*",
        "2amigos/yii2-date-picker-widget": "*",
        "2amigos/yii2-date-time-picker-widget": "*",
        "2amigos/yii2-multi-select-widget": "*",
        "2amigos/yii2-editable-widget": "*",
        "rmrevin/yii2-fontawesome": "*",
        "speixoto/yii2-amcharts": "*",
        "hiqdev/yii2-asset-autosize": "*",
        "kartik-v/yii2-grid": "*",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-widget-fileinput": "*",
        "kartik-v/yii2-widget-typeahead": "*",
        "kartik-v/yii2-date-range": "dev-master",
        "kartik-v/yii2-markdown": "dev-master",
        "kartik-v/yii2-widget-touchspin": "*",
        "kartik-v/yii2-context-menu": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "wfcreations\\metronic\\": ""
        }
    }
}
